"""Exposes a URL that specifies the behavior of this scalar."""
directive @specifiedBy(
  """The URL that specifies the behavior of this scalar."""
  url: String!
) on SCALAR

type HelloAuth {
  auth: String!
}

type HelloChat {
  chat: String!
}

type HelloGame {
  game: String!
}

type HelloWorld {
  world: String!
}

type Query {
  helloAuth: HelloAuth!
  helloChat: HelloChat!
  helloWorld: HelloWorld!
  helloGame: HelloGame!
}

